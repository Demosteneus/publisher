<?xml version="1.0" encoding="UTF-8"?>
<Layout xmlns="urn:speedata.de:2009/publisher/en" xmlns:sd="urn:speedata:2009/publisher/functions/en">
  <Options show-grid="yes"/>
  
  <Record element="data">
    <SetVariable variable="img" select="'ocean.pdf'"/>
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>Mit ausschnitt="nein" (default) und maxbreite und maxhöhe</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>maxbreite="5" maxhöhe="5"</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Image maxwidth="5" maxheight="5" file="{$img}" clip="no"/>
    </PlaceObject>
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>maxbreite="10" maxhöhe="5"</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Image maxwidth="10" maxheight="5" file="{$img}" clip="no"/>
    </PlaceObject>
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>maxbreite="5" maxhöhe="10"</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Image maxwidth="5" maxheight="10" file="{$img}" clip="no"/>
    </PlaceObject>
    <NewPage></NewPage>
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>Mit ausschnitt="nein"</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>breite="5" höhe="5"</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Image width="5" height="5" file="{$img}" clip="no"/>
    </PlaceObject>
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>breite="10" höhe="5"</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Image width="10" height="5" file="{$img}" clip="no"/>
    </PlaceObject>
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>breite="5" höhe="10"</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Image width="5" height="10" file="{$img}" clip="no"/>
    </PlaceObject>
    <NewPage/>
    
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>Mit ausschnitt="ja"</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>breite="5" höhe="5"</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Image width="5" height="5" file="{$img}" clip="yes"/>
    </PlaceObject>
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>breite="10" höhe="5"</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Image width="10" height="5" file="{$img}" clip="yes"/>
    </PlaceObject>
    <PlaceObject>
      <Textblock>
        <Paragraph><Value>breite="5" höhe="10"</Value></Paragraph>
      </Textblock>
    </PlaceObject>
    <PlaceObject>
      <Image width="5" height="10" file="{$img}" clip="yes"/>
    </PlaceObject>
  </Record>
</Layout>
