<?xml version="1.0" encoding="UTF-8"?>
<Layout xmlns="urn:speedata.de:2009/publisher/en" xmlns:sd="urn:speedata:2009/publisher/functions/en">
	<Pageformat width="6cm" height="14cm" />
	<SetGrid height="12pt" nx="1"/>

	<Record element="data">
		<Switch>
			<Case test="a">
				<PlaceObject>
					<Textblock>
						<Paragraph>
							<Value>a exists</Value>
						</Paragraph>
					</Textblock>
				</PlaceObject>
			</Case>
		</Switch>
		<Switch>
			<Case test="not(b)">
				<PlaceObject>
					<Textblock>
						<Paragraph>
							<Value>b does not exist</Value>
						</Paragraph>
					</Textblock>
				</PlaceObject>
			</Case>
		</Switch>
	</Record>
</Layout>