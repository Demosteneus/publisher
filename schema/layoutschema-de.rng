   <define name="e_PlaceObject">
      <element name="PlaceObject">
         <a:documentation>Gibt ein rechteckiges Objekt (Bild, Tabelle, Box, Barcode oder Textblock) aus.</a:documentation>
         <interleave>
            <group>
               <optional>
                  <attribute name="allocate">
                     <choice>
                        <value>yes</value>
                        <a:documentation>Platz im Raster reservieren (Voreinstellung für Platzierung im Raster).</a:documentation>
                        <value>no</value>
                        <a:documentation>Bereich nicht als Belegt markieren (Voreinstellung für absolute Positionierung)</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="allocate-left"></attribute>
               </optional>
               <optional>
                  <attribute name="allocate-right"></attribute>
               </optional>
               <optional>
                  <attribute name="allocate-top"></attribute>
               </optional>
               <optional>
                  <attribute name="allocate-bottom"></attribute>
               </optional>
               <optional>
                  <attribute name="area"></attribute>
               </optional>
               <optional>
                  <attribute name="groupname"></attribute>
               </optional>
               <optional>
                  <attribute name="border-bottom-right-radius"></attribute>
               </optional>
               <optional>
                  <attribute name="border-top-right-radius"></attribute>
               </optional>
               <optional>
                  <attribute name="border-top-left-radius"></attribute>
               </optional>
               <optional>
                  <attribute name="border-bottom-left-radius"></attribute>
               </optional>
               <optional>
                  <attribute name="frame">
                     <choice>
                        <value>solid</value>
                        <a:documentation>Einen Rahmen um das Objekt zeichnen.</a:documentation>
                        <value>without</value>
                        <a:documentation>Keinen Rahmen zeichnen.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="framecolor"></attribute>
               </optional>
               <optional>
                  <attribute name="background">
                     <choice>
                        <value>full</value>
                        <a:documentation>Den Hintergrund in der angegebenen Farbe füllen (in backgroundcolor)</a:documentation>
                        <value>without</value>
                        <a:documentation>Leerer Hintergrund</a:documentation>
                        <data type="string">
                           <param name="pattern">\{.+\}</param>
                        </data>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="backgroundcolor"></attribute>
               </optional>
               <optional>
                  <attribute name="rulewidth"></attribute>
               </optional>
               <optional>
                  <attribute name="column"></attribute>
               </optional>
               <optional>
                  <attribute name="keepposition">
                     <choice>
                        <value>yes</value>
                        <a:documentation>Den virtuellen Cursor nicht verändern.</a:documentation>
                        <value>no</value>
                        <a:documentation>Den virtuellen Cursor verschieben (Voreinstellung)</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="halign">
                     <choice>
                        <value>left</value>
                        <a:documentation>Das Objekt wird links ausgerichtet.</a:documentation>
                        <value>center</value>
                        <a:documentation>Das Objekt wird so platziert, dass der Leerraum links und rechts derselbe ist.</a:documentation>
                        <value>right</value>
                        <a:documentation>Das Objekt wird rechts ausgerichtet.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="page"></attribute>
               </optional>
               <optional>
                  <attribute name="origin-x">
                     <choice>
                        <value>left</value>
                        <a:documentation>Der Ursprung liegt links.</a:documentation>
                        <value>center</value>
                        <a:documentation>Der Ursprung liegt im Mittelpunkt des Objekts.</a:documentation>
                        <value>right</value>
                        <a:documentation>Der Ursprung liegt rechts.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="origin-y">
                     <choice>
                        <value>top</value>
                        <a:documentation>Rotation um die obere Kante des Objekts.</a:documentation>
                        <value>center</value>
                        <a:documentation>Rotation um die vertikale Mitte.</a:documentation>
                        <value>bottom</value>
                        <a:documentation>Um einen Punkt am unteren Rand rotieren.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="row"></attribute>
               </optional>
               <optional>
                  <attribute name="rotate"></attribute>
               </optional>
               <optional>
                  <attribute name="maxheight"></attribute>
               </optional>
               <optional>
                  <attribute name="valign">
                     <choice>
                        <value>top</value>
                        <a:documentation>Das Objekt wird oben ausgerichtet.</a:documentation>
                        <value>middle</value>
                        <a:documentation>Das Objekt wird so platziert, dass der Leerraum oberhalb und unterhalb derselbe ist.</a:documentation>
                        <value>bottom</value>
                        <a:documentation>Das Objekt wird unten ausgerichtet.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="hreference">
