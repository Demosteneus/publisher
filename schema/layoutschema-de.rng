   <define name="e_Barcode">
      <element name="Barcode">
         <a:documentation>Erzeugt einen 1D oder 2D Strichcode (barcode), der in PlaceObject ausgegeben werden kann.</a:documentation>
         <interleave>
            <group>
               <attribute name="select"></attribute>
               <optional>
                  <attribute name="fontface"></attribute>
               </optional>
               <attribute name="type">
                  <choice>
                     <value>QRCode</value>
                     <a:documentation>Erzeugt einen QR-Code, der für den Inhalt die kleinstmögliche Größe und den besten Fehlerkorrekturwert hat, sofern kein Fehlerkorrekturlevel angegeben ist.</a:documentation>
                     <value>Code128</value>
                     <a:documentation>Erzeugt einen Code 128 Barcode für Ziffern und Text (ohne Umlaute)</a:documentation>
                     <value>EAN13</value>
                     <a:documentation>Erzeugt einen EAN13 Barcode mit genau 13 Ziffern.</a:documentation>
                  </choice>
               </attribute>
               <optional>
                  <attribute name="width"></attribute>
               </optional>
               <optional>
                  <attribute name="height"></attribute>
               </optional>
               <optional>
                  <attribute name="showtext">
                     <choice>
                        <value>yes</value>
                        <a:documentation>Text unterhalb des Barcodes schreiben.</a:documentation>
                        <value>no</value>
                        <a:documentation>Keinen Text anzeigen.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="overshoot"></attribute>
               </optional>
               <optional>
                  <attribute name="eclevel">
                     <choice>
                        <value>L</value>
