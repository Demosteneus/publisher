         <interleave>
            <group>
               <optional>
                  <attribute name="select"></attribute>
               </optional>
               <optional>
                  <attribute name="error">
                     <choice>
                        <value>yes</value>
                        <a:documentation>Report an error.</a:documentation>
                        <value>no</value>
                        <a:documentation>Do not report an error (default).</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="errorcode"></attribute>
               </optional>
               <optional>
                  <attribute name="exit">
                     <choice>
                        <value>no</value>
                        <a:documentation>The speedata Publisher continues with the PDF creation.</a:documentation>
                        <value>yes</value>
                        <a:documentation>The speedata Publisher exits without finishing the PDF file.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <oneOrMore>
                     <ref name="e_Value"></ref>
                  </oneOrMore>
               </optional>
            </group>
            <ref name="foreign-nodes"></ref>
         </interleave>
      </element>
   </define>
