                        <value>top</value>
                        <a:documentation>If &#39;top&#39; (default), the given row is the top border of the object.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <oneOrMore>
                  <interleave>
                     <optional>
                        <ref name="e_Image"></ref>
                     </optional>
                     <optional>
                        <ref name="e_Box"></ref>
                     </optional>
                     <optional>
                        <ref name="e_Circle"></ref>
                     </optional>
