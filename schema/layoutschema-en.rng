               </optional>
               <optional>
                  <attribute name="border-bottom-left-radius"></attribute>
               </optional>
               <optional>
                  <attribute name="frame">
                     <choice>
                        <value>solid</value>
                        <a:documentation>Draw a frame around the object.</a:documentation>
                        <value>without</value>
                        <a:documentation>Don&#39;t draw a frame around the object.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="framecolor"></attribute>
               </optional>
               <optional>
                  <attribute name="background">
                     <choice>
                        <value>full</value>
                        <a:documentation>Fill background with the given color (in backgroundcolor).</a:documentation>
                        <value>without</value>
                        <a:documentation>Empty background</a:documentation>
                        <data type="string">
                           <param name="pattern">\{.+\}</param>
                        </data>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="backgroundcolor"></attribute>
               </optional>
               <optional>
                  <attribute name="rulewidth"></attribute>
               </optional>
               <optional>
                  <attribute name="column"></attribute>
               </optional>
               <optional>
                  <attribute name="keepposition">
                     <choice>
                        <value>yes</value>
                        <a:documentation>Don&#39;t move the virtual cursor.</a:documentation>
                        <value>no</value>
                        <a:documentation>Move the virtual cursor (default).</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="halign">
                     <choice>
                        <value>left</value>
                        <a:documentation>The object is aligned at the left.</a:documentation>
                        <value>center</value>
                        <a:documentation>The object is aligned so that the space to the left is the same as to the right.</a:documentation>
                        <value>right</value>
                        <a:documentation>The object is aligned to the right.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="page"></attribute>
               </optional>
               <optional>
                  <attribute name="origin-x">
                     <choice>
                        <value>left</value>
                        <a:documentation>The origin is at the left hand side.</a:documentation>
                        <value>center</value>
                        <a:documentation>The origin is in the center of the object.</a:documentation>
                        <value>right</value>
                        <a:documentation>The origin is at the right hand side.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="origin-y">
                     <choice>
                        <value>top</value>
                        <a:documentation>Rotation around a point at the top.</a:documentation>
                        <value>center</value>
                        <a:documentation>Rotate around the vertical center.</a:documentation>
                        <value>bottom</value>
                        <a:documentation>Rotate around a point at the bottom.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="row"></attribute>
               </optional>
               <optional>
                  <attribute name="rotate"></attribute>
               </optional>
               <optional>
                  <attribute name="maxheight"></attribute>
               </optional>
               <optional>
                  <attribute name="valign">
                     <choice>
                        <value>top</value>
                        <a:documentation>The object is aligned at the top.</a:documentation>
                        <value>middle</value>
                        <a:documentation>The object is aligned so that the space at the top is the same as at the bottom.</a:documentation>
                        <value>bottom</value>
                        <a:documentation>The object is aligned at the bottom.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="hreference">
                     <choice>
                        <value>left</value>
                        <a:documentation>The object is placed in given column.</a:documentation>
                        <value>right</value>
                        <a:documentation>The given columns determines the right edge of the border.</a:documentation>
                     </choice>
