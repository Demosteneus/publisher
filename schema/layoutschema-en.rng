   <define name="e_DefineColor">
      <element name="DefineColor">
         <a:documentation>Colors defined with DefineColors can be referenced later by their name.</a:documentation>
         <optional>
            <attribute name="model">
               <a:documentation>Color model to be used for the color. Currently “rgb”, “cmyk” and “gray” are supported. More on request.</a:documentation>
               <choice>
                  <value>cmyk</value>
                  <a:documentation xmlns="">CMYK (cyan, magenta, yellow, key/black)</a:documentation>
                  <value>rgb</value>
                  <a:documentation xmlns="">RGB (red, green, blue)</a:documentation>
                  <value>gray</value>
                  <a:documentation xmlns="">Gray (0=Black)</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="name">
               <a:documentation>The name of the color to be defined.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="r">
               <a:documentation>Red part with RGB</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="g">
               <a:documentation>Green part with RGB / gray part when using the model gray</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="b">
               <a:documentation>Blue part with RGB</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="c">
               <a:documentation>Cyan part with CMYK</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="m">
               <a:documentation>Magenta part with CMYK</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="y">
               <a:documentation>Yellow part with CMYK</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="k">
               <a:documentation>Black part with CMYK</a:documentation>
            </attribute>
         </optional>
