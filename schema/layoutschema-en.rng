   <define name="e_Barcode">
      <element name="Barcode">
         <a:documentation>Print a 1d or 2d barcode. To be used in PlaceObject.</a:documentation>
         <interleave>
            <group>
               <attribute name="select"></attribute>
               <optional>
                  <attribute name="fontface"></attribute>
               </optional>
               <attribute name="type">
                  <choice>
                     <value>QRCode</value>
                     <a:documentation>Create an »optimal« QR code in terms of error correction and size.</a:documentation>
                     <value>Code128</value>
                     <a:documentation>Generate a code 128 barcode for numbers and text.</a:documentation>
                     <value>EAN13</value>
                     <a:documentation>Create an EAN13 barcode for 13 digits.</a:documentation>
                  </choice>
               </attribute>
               <optional>
                  <attribute name="width"></attribute>
               </optional>
               <optional>
                  <attribute name="height"></attribute>
               </optional>
               <optional>
                  <attribute name="showtext">
                     <choice>
                        <value>yes</value>
                        <a:documentation>Write text beneath the barcode.</a:documentation>
                        <value>no</value>
                        <a:documentation>Don&#39;t display text.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="overshoot"></attribute>
               </optional>
               <optional>
                  <attribute name="eclevel">
                     <choice>
                        <value>L</value>
                        <a:documentation>Set the lowest level (1) with approx. 7% recovery.</a:documentation>
                        <value>M</value>
                        <a:documentation>Set the second lowest level (2) with approx. 15% recovery.</a:documentation>
                        <value>Q</value>
                        <a:documentation>Set the second highest level (3) with approx. 25% recovery.</a:documentation>
                        <value>H</value>
                        <a:documentation>Set the highest level (4) with approx. 35% recovery.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <empty></empty>
            </group>
            <ref name="foreign-nodes"></ref>
         </interleave>
      </element>
