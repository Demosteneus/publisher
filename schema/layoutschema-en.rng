   <define name="e_DefineTextformat">
      <element name="DefineTextformat">
         <a:documentation>Define text formatting instructions. A textformat is used to align and indent text and create margins and rules before and after the text.</a:documentation>
         <interleave>
            <group>
               <optional>
                  <attribute name="hyphenchar"></attribute>
               </optional>
               <optional>
                  <attribute name="orphan">
                     <choice>
                        <value>yes</value>
                        <a:documentation>Allow orphans.</a:documentation>
                        <value>no</value>
                        <a:documentation>Prevent orphans (default).</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="widow">
                     <choice>
                        <value>yes</value>
                        <a:documentation>Allow widows.</a:documentation>
                        <value>no</value>
                        <a:documentation>Prevent widows (default).</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <attribute name="name"></attribute>
               <optional>
                  <attribute name="alignment">
                     <choice>
                        <value>justified</value>
                        <a:documentation>Textblock has a rectangular shape.</a:documentation>
                        <value>leftaligned</value>
                        <a:documentation>The text is ragged at the right margin.</a:documentation>
                        <value>rightaligned</value>
                        <a:documentation>The text is ragged right at the left margin.</a:documentation>
                        <value>centered</value>
                        <a:documentation>The text is ragged at the left and the right margin.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="indentation"></attribute>
               </optional>
               <optional>
                  <attribute name="rows"></attribute>
               </optional>
               <optional>
                  <attribute name="border-top"></attribute>
               </optional>
               <optional>
                  <attribute name="border-bottom"></attribute>
               </optional>
               <optional>
                  <attribute name="margin-bottom"></attribute>
               </optional>
               <optional>
                  <attribute name="break-below">
                     <choice>
                        <value>yes</value>
                        <a:documentation>Allow a break below this text (default).</a:documentation>
                        <value>no</value>
                        <a:documentation>Prevent a page break below this text.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="padding-top"></attribute>
               </optional>
               <optional>
                  <attribute name="margin-top"></attribute>
               </optional>
               <optional>
                  <attribute name="hyphenate">
                     <choice>
                        <value>yes</value>
                        <a:documentation>Enable hyphenation (default).</a:documentation>
                        <value>no</value>
                        <a:documentation>Disable hyphenation.</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <optional>
                  <attribute name="tab">
                     <choice>
                        <value>space</value>
                        <a:documentation>Use tab as space</a:documentation>
                        <value>hspace</value>
                        <a:documentation>Use tab as a stretching space</a:documentation>
                     </choice>
                  </attribute>
               </optional>
               <empty></empty>
            </group>
            <ref name="foreign-nodes"></ref>
         </interleave>
      </element>
