   <define name="e_DefineTextformat">
      <element name="DefineTextformat">
         <a:documentation>Define text formatting instructions. A textformat is used to align and indent text and create margins and rules before and after the text.</a:documentation>
         <attribute name="name">
            <a:documentation>Name of the textformat that is used later in the layout.</a:documentation>
         </attribute>
         <optional>
            <attribute name="alignment">
               <a:documentation>Determines the formatting of the text.</a:documentation>
               <choice>
                  <value>justified</value>
                  <a:documentation xmlns="">Textblock has a rectangular shape.</a:documentation>
                  <value>leftaligned</value>
                  <a:documentation xmlns="">The text is ragged at the right margin.</a:documentation>
                  <value>rightaligned</value>
                  <a:documentation xmlns="">The text is ragged right at the left margin.</a:documentation>
                  <value>centered</value>
                  <a:documentation xmlns="">The text is ragged at the left and the right margin.</a:documentation>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="indentation">
               <a:documentation>The amount of indentation.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="rows">
               <a:documentation>The number of rows with indentation given in the attribute indentation. If the number is negative, this determiens the number of rows that are not indented.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-top">
               <a:documentation>The thickness of the rule above the text.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="border-bottom">
               <a:documentation>The thickness of the rule below the text.</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="margin-bottom">
               <a:documentation/>
            </attribute>
         </optional>
         <optional>
            <attribute name="break-below">
               <a:documentation>(Dis-)Allow break below the text.</a:documentation>
               <choice>
                  <value>yes</value>
                  <value>no</value>
                  <text/>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="padding-top">
               <a:documentation/>
            </attribute>
         </optional>
         <optional>
            <attribute name="margin-top">
               <a:documentation/>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
