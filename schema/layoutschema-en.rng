         </optional>
         <optional>
            <attribute name="fontface">
               <a:documentation>The name of the fontface for the paragraph. The default is »text« (lowercase t).</a:documentation>
            </attribute>
         </optional>
         <optional>
            <attribute name="language">
               <a:documentation>Name of the language for hyphenation.</a:documentation>
               <choice>
                  <value>German</value>
                  <a:documentation xmlns="">Use German language for hyphenation.</a:documentation>
                  <value>Englisch (Great Britan)</value>
                  <a:documentation xmlns="">Use English hyphenation patterns</a:documentation>
                  <value>French</value>
                  <a:documentation xmlns="">French hyphenation patterns</a:documentation>
               </choice>
            </attribute>
         </optional>
         <zeroOrMore>
            <choice>
               <ref name="e_Action"/>
               <ref name="e_A"/>
